<style>
#datatable {
  font-family: Arial, Helvetica, sans-serif;
  border-collapse: collapse;
  width: 100%;
}
#datatable td, #datatable th {
  border: 1px solid #ddd;
  padding: 8px;
}
#datatable tr:nth-child(even){background-color: #f2f2f2;}
#datatable tr:hover {background-color: #ddd;}
#datatable th {
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: left;
  background-color: #4CAF50;
  color: white;
}
</style>
<table id="datatable">
	<thead>
		<th>Address</th>
		<th>Date</th>
		<th>Age</th>
		<th>Capacity</th>
		<th>Slots</th>
		<th>Fees</th>
		<th>Vaccine</th>
	</thead>
	{{#each data}}

	{{#each this}}
	<tr>
		<td>{{this.address}}</td>
		<td>{{this.date}}</td>
		<td>{{this.ageLimit}}</td>
		<td>{{this.capacity}}</td>
		<td>{{this.slots}}</td>
		<td>{{this.fees}}</td>
		<td>{{this.vaccine}}</td>
	</tr>
	<tr />
	{{/each}}

	{{/each}}
</table>